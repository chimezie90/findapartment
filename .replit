run = "python -m apartment_finder.web.app"
entrypoint = "src/apartment_finder/web/app.py"
modules = ["python-3.12", "web", "nix"]

[env]
PYTHONPATH = "${REPL_HOME}/src"

[nix]
channel = "stable-25_05"
packages = ["libyaml"]

[deployment]
run = ["python", "-m", "apartment_finder.web.app"]
deploymentTarget = "cloudrun"

[[ports]]
localPort = 5000
externalPort = 80

[agent]
expertMode = true
